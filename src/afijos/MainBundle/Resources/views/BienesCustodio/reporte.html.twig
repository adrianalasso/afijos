<div class="page-content">
    <div class="page-header position-relative">
        <h1>Bienes en Custodio<small><i class="icon-double-angle-right"></i> Reporte de Bienes</small></h1>
    </div><!--/.page-header-->
    <div class="row-fluid">
        <div class="span10">
            <!--PAGE CONTENT BEGINS-->                
            {%if entities%}       
            <table  id="sample-table-2" class="table table-striped table-bordered table-hover">
                <thead>
                    <tr>
                        <th>Código</th>
                        <th>Descripción</th>
                        <th>Responsable</th>
                        <th>Marca</th>
                        <th>Oficina</th>
                        <th>Dirección</th>
                        <th>Departamento</th>
                        <th>Sección</th>
                        <th>Componentes</th>
                        <th>Serie</th>
                    </tr>
                 </thead>                
                <tbody>
                    {% for entity in entities %}
                        <tr>
                            <td>{{ entity.codigo }}</td>
                            <td>{{ entity.descripcion }}</td>
                            <td>{{ entity.trabajador}}</td>
                            <td>{{ entity.marca }}</td>
                            <td>{{ entity.oficina }}</td>
                            <td>{{ entity.direccion }}</td>
                            <td>{{ entity.depart }}</td>
                            <td>{{ entity.seccion }}</td>
                            <td>{{ entity.componentes }}</td>
                            <td>{{ entity.serie }}</td>
                        </tr>
                    {% endfor %}
                </tbody>                
            </table>
            {%else%}
            <div class="alert alert-error"> 
                <strong><i class="icon-remove"></i>  Opps!</strong>
                No se han encontrado items para esta línea.
                <br>
            </div>
            {%endif%}
            <div class="hr hr32 hr-dotted"></div>
            <!--PAGE CONTENT ENDS-->
        </div><!--/.span-->
    </div><!--/.row-fluid-->
</div><!--/.page-content-->