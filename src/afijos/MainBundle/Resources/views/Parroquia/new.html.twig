{%block formulario%}
<form action="{{ path('parroquia_create') }}" method="post" {{ form_enctype(form) }}>

<div class="control-group">
        <label class="control-label" for="">
            Nombre
        </label>
        <div class="controls">
                {{ form_widget(form.nombre) }}
        </div>
</div>
                {{ form_rest(form) }}
