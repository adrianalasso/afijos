{% extends "MainBundle::new.html.twig" %}
{% block titulo %}Dependencias{%endblock%}

{%block entidad%}
<a href="{{ path('dependencia') }}">Dependencias</a>
{%endblock%}

{%block mapatitulo%}Nueva Dependencia{%endblock%}
{%block pagetitulo%}Dependencias{%endblock%}
{%block pagedetalle%}Nueva Dependencia{%endblock%}

{%block formulario%}
<form action="{{ path('dependencia_create') }}" method="post" {{ form_enctype(form) }}>
<div class="control-group">
        <label class="control-label" for="">
            Código
        </label>
        <div class="controls">
                {{ form_widget(form.id) }}
        </div>
</div>
<div class="control-group">
        <label class="control-label" for="">
            Descripción
        </label>
        <div class="controls">
                {{ form_widget(form.descripcion) }}
        </div>
</div>
                {{ form_rest(form) }}
{%endblock%}
{%block link%}
<a href="{{ path('dependencia') }}"  
{%endblock%}

{%block scripthijo%}
<script type="text/javascript">
    window.onload = function(){
        var elemento=document.getElementById("mndependencias");
        elemento.classList.add('active');
        elemento.parentNode.parentNode.classList.add('active');
        elemento.parentNode.parentNode.classList.add('open');
    };
</script>
{%endblock%}



