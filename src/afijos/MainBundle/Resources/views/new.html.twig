{% extends '::base.html.twig' %}

{% block titulo %}{%endblock%}

{% block mapasitio%}
<div class="breadcrumbs" id="breadcrumbs">
    <ul class="breadcrumb">
        <li>
            <i class="icon-home home-icon"></i>
            <a href="{{path('usuario_index')}}">Inicio</a>
            <span class="divider"><i class="icon-angle-right arrow-icon"></i></span>
        </li>
        <li>
            <i class="icon-cogs cogs-icon"></i>
            <a href="#">Configuración</a>
            <span class="divider"><i class="icon-angle-right arrow-icon"></i></span>
        </li>
        <li>
            {%block entidad%}
            <i class="icon-bolt bolt-icon"></i>
            <a href="#">Subestaciones</a>
            {%endblock%}
            <span class="divider"><i class="icon-angle-right arrow-icon"></i></span>
        </li>        
        <li class="active">{%block mapatitulo%}Nueva Subestación{%endblock%}</li>             
    </ul><!--.breadcrumb-->
</div>
{%endblock%}

{% block pagecontent%}
<div class="page-content">
    <div class="row-fluid">
        <div class="span6">            
            <!--PAGE CONTENT BEGINS-->
            <div class="widget-box">
                <div class="widget-header">
                        <h4>{%block pagetitulo%}Subestaciones{%endblock%}
                            <small><i class="icon-double-angle-right"></i> {%block pagedetalle%}Nueva Subestación{%endblock%}</small>
                        </h4>        
                </div>

                <div class="widget-body">
                        <div class="widget-main">
                                <div class="form-horizontal">
                                    {%block formulario%}
                                    <form action="{{ path('subestacion_create') }}" method="post" {{ form_enctype(form) }}>
                                        {%endblock%}
                                        <div class="well well-small"> 
                                            <div class="center">
                                                <button class="btn btn-small btn-success " type="submit">
                                                    <i class="icon-save bigger-150"></i>
                                                    Crear
                                                </button>                                                    
                                             {%block link%}
                                                <a href="{{ path('subestacion') }}"  
                                                {%endblock%}
                                                   class="btn btn-small btn-grey">
                                                    <i class="icon-undo bigger-150"></i>
                                                    Regresar
                                                </a>     
                                            </div>
                                        </div>
                                    </form>
                                </div>
                        </div>
                    </div>
                </div>            
            <!--PAGE CONTENT ENDS-->
        </div><!--/.span-->
    </div><!--/.row-fluid-->
</div><!--/.page-content-->
{%endblock%}

{%block plugins%}{%endblock%}

{%block script%}
{{parent()}}
{%block scripthijo%}{%endblock%}
{%endblock%}

